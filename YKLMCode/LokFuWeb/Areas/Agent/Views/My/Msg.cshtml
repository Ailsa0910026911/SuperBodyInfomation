@{
    IList<MsgAdmin> MsgAdminList = ViewBag.MsgAdminList;
    ViewBag.SMUid = 155;
    string IsDel = Request.QueryString["IsDel"] != null ? Request.QueryString["IsDel"].ToString() : "0";
    ViewBag.Title = "查看消息";
}
<body class="index inquiry order">
    <div class="main-content clearfix">
        <div class="ifarm-index">
            <div class="inquiry-cont"></div>
            <div class="list-table">
                <table style="width: 100%">
                    <thead>
                        <th width="70%">标题 </th>
                        <th width="10%">阅读 </th>
                        <th width="20%">时间 </th>
                    </thead>
                    <tbody>
                        @foreach (var p in MsgAdminList)
                        {
                            <tr class="odd gradeX">
                                <td><a href="/Agent/My/MsgInfo.html?Id=@(p.Id)" class="Ajax">@(p.Name)</a></td>
                                <td class="center">@if (p.State == 1)
                                                   {
                                    <i class="fa fa-envelope color-yellow">新</i>
                                                   }
                                                   else
                                                   {
                                    <i class="fa fa-envelope-o">阅</i>
                                                   } </td>
                                <td class="center">@(p.AddTime.ToString("yyyy-MM-dd HH:mm")) </td>
                            </tr>
                        }
                    </tbody>
                </table>
                @Html.Partial("/Areas/Agent/Views/Shared/NewPage.cshtml", MsgAdminList)
            </div>
        </div>
    </div>
</body>
@section FooterScripts
{
    @{
        <script>
            $(function () {
               
                $(".aui_state_lock ").on("click", ".aui_close", function () {
                    console.log("111111111111");
                });
            });
        </script>
    }
}

