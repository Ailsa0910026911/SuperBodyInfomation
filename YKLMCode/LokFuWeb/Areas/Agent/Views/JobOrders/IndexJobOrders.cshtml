@{
    List<JobItem> JobItemList = ViewBag.JobItemList;
    this.Layout = null;
}
<table class="table table-striped table-bordered table-hover" id="JobOrders">
    <thead>
        <tr>
            @*<th>订单号</th>*@
            <th style="text-align:center">编号</th>
            <th style="text-align:center">金额</th>
            <th style="text-align:center">执行时间</th>
   @*         <th style="text-align:center">手续费</th>
            <th style="text-align:center">分润</th>*@
            <th style="text-align:center">计划状态</th>
            <th style="text-align:center">类型</th>
           @* <th style="text-align:center">结果</th>
            <th style="text-align:center">支付时间</th>
            <th style="text-align:center">结束时间</th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var item in JobItemList)
        {
            <tr class="odd gradeX" style="@(item.RunType==2?"color:red":"")">
                @*<td>@(item.TNum)</td>*@
                <td style="text-align:center">@(item.RunNum)</td>
                <td style="text-align:center">@(item.RunMoney.ToString("F2"))</td>
                <td style="text-align:center">@(item.RunTime.ToString("yyyy-MM-dd HH:mm"))</td>
           @*     <td style="text-align:center">@(item.Poundage.ToString("F2"))</td>*@
                <td style="text-align:center">
                    @switch (item.State)
                    {
                        case 0:
                        @("取消")
                            break;
                        case 1:
                        @("待执行")
                            break;
                        case 2:
                        @("执行中")
                            break;
                        case 3:
                        @("执行完成")
                            break;
                        case 4:
                        @("执行失败")
                            break;
                    }
                </td>
                <td style="text-align:center">@if (item.RunType == 1)
                    { @("消费")}
                    else if (item.RunType == 2)
                    { 
                    @("还款")
                    }
                </td>
               @* <td style="text-align:center">@(item.AgentGet.ToString("F2"))</td>*@
           @*     <td style="text-align:center">
                    @if (item.RunType == 2)
                    {
                        switch (item.RunState)
                        {
                            case 0:
                        @("未付")
                                break;
                            case 1:
                        @("已付")
                                break;
                        }
                    }
                    else if (item.RunType == 1)
                    {
                        switch (item.RunState)
                        {
                            case 0:
                        @("未付")
                                break;
                            case 1:
                        @("成功 ")
                                break;
                            case 2:
                        @("处理中 ")
                                break;
                            case 3:
                        @("失败")
                                break;
                        }
                    }
                </td>
                <td style="text-align:center">@(item.RunedTime.HasValue ? item.RunedTime.Value.ToString("yyyy-MM-dd HH:mm") : "--")</td>
                <td style="text-align:center">@(item.AddTime.HasValue ? item.AddTime.Value.ToString("yyyy-MM-dd HH:mm") : "--")</td>*@
            </tr>
        }
    </tbody>
</table>
<link href="/Content/Metronic/plugins/artdialog/skins/default.css" rel="stylesheet" type="text/css" />
<script src="/Content/Metronic/plugins/artdialog/jquery.artDialog.js" type="text/javascript"></script>
<script src="/Content/Metronic/plugins/artdialog/iframeTools.js" type="text/javascript"></script>
<script src="/Content/Metronic/scripts/custom/public.js" type="text/javascript"></script>
