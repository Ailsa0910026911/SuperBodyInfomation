<div class="row">
    <div class="col-md-12">
        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet box light-grey">

            <div class="portlet-title">
                <div class="caption"><i class="fa fa-globe"></i>@(ViewBag.Title)</div>
            </div>

            <div class="portlet-body">
                <div class="table-toolbar">
                    <div class="btn-group pull-right" style="margin-bottom:10px">
                    </div>

                    <div class="table-toolbar">
                            <div class="btn-group pull-right">
                                <a class="btn Ajax" href="Add.html"><i class="fa fa-wrench"></i>添加</a>
                            </div>
                        </div>
                    <div class="table-scrollable">
                        <table class="table table-striped table-bordered table-hover tablethc" id="ListBox">
                            <thead>
                                <tr>
                                    <th>接口名称 </th>
                                    <th>显示名称</th>
                                    <th>组件 </th>
                                    <th>时间限制</th>
                                    <th>限制时间段</th>
                                    <th>结算费用</th>
                                    <th>状态</th>
                                    <th>排序</th>
                                    <th>类型</th>
                                    <th>支持</th>
                                    <th>开启</th>
                                    <th>入网费率</th>
                                    @*  <th>代理商费率</th>*@
                                    <th>成本费率</th>
                                    <th>手续费</th>
                                    <th>限额</th>
                                    <th width="100">操作</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (FastPayWayList != null && FastPayWayList.Count() > 0)
                                {
                                    foreach (var p in FastPayWayList)
                                    {
                                        <tr class="odd gradeX">
                                            <td class="center" style="text-align:center;vertical-align:middle;" rowspan="4">@(p.Title) </td>
                                            <td class="center" style="text-align:center;vertical-align:middle;" rowspan="4">@(p.ShowName)</td>
                                            <td class="center" style="text-align:center;vertical-align:middle;" rowspan="4">@(p.DllName)(@(p.GroupType)) </td>
                                            <td class="center" style="text-align:center;vertical-align:middle;" rowspan="4">
                                                @if (p.TimeType == 0)
                                                {
                                                    @("不限制")
                                                }
                                                else
                                                {
                                                    <i class="color-green">按天</i>
                                                }
                                            </td>
                                            <td class="center" style="text-align:center;vertical-align:middle;" rowspan="4">
                                                @(p.STime.ToString("HH:mm")) -
                                                @( p.ETime.ToString("HH:mm"))
                                            </td>
                                            <td class="center" style="text-align:center;vertical-align:middle;" rowspan="4"><span class="permillage">@(p.Cash.ToMoney())</span>元</td>
                                            <td class="center" style="text-align:center;vertical-align:middle;" rowspan="4">
                                                @if (p.State == 1)
                                                {
                                                    <i class="fa fa-check color-green"></i>
                                                }
                                                else
                                                {
                                                    <i class="fa fa-times color-red"></i>
                                                }
                                            </td>
                                            <td class="center" rowspan="4" style="border: 1px solid #ddd;text-align:center;vertical-align:middle;">@(p.Sort) </td>
                                        </tr>
                                        <tr>
                                            <td class="center">支付宝</td>
                                            <td class="center">
                                                @if (p.CanOpenAliPay == 1)
                                                {
                                                    <i class="fa fa-check color-green"></i>
                                                }
                                                else
                                                {
                                                    <i class="fa fa-times color-red"></i>
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenAliPay == 1)
                                                {
                                                    if (p.HasAliPay == 1)
                                                    {
                                                        <i class="fa fa-check color-green"></i>
                                                    }
                                                    else
                                                    {
                                                        <i class="fa fa-times color-red"></i>
                                                    }
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenAliPay == 1)
                                                {
                                                    <span class="permillage">@( (p.InCost2 * 1000).ToString("f2"))</span>@:‰
                                            }
                                            </td>
                                            @* <td class="center">
                                                    @if (p.CanOpenAliPay == 1)
                                                    {
                                                        <span class="permillage">@( (p.Cost2 * 1000).ToString("f2"))</span>@:‰
                                                    }
                                                </td>*@
                                            <td class="center">
                                                @if (p.CanOpenAliPay == 1)
                                                {
                                                    <span class="permillage">@( (p.BankCost2 * 1000).ToString("f2"))</span>@:‰
                                            }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenAliPay == 1)
                                                {
                                                    @(p.MinCost2.ToMoney() + " - " + p.MaxCost2.ToMoney())
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenAliPay == 1)
                                                {
                                                    @(p.SNum2.ToMoney() + " - " + p.ENum2.ToMoney())
                                                }
                                            </td>
                                            <td class="center" rowspan="4" style="text-align:center;vertical-align:middle;">
                                                @if (ViewBag.Save)
                                                {
                                                    <a class="label label-sm label-success Ajax" href="Edit.html?Id=@(p.Id)"><i class="fa fa-wrench"></i>修改</a>
                                                    <a class="label label-sm label-danger Ajax" href="Delete.html?Id=@(p.Id)"><i class="fa fa-wrench"></i>删除</a>
                                                }

                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="center">微信</td>
                                            <td class="center">
                                                @if (p.CanOpenWeiXin == 1)
                                                {
                                                    <i class="fa fa-check color-green"></i>
                                                }
                                                else
                                                {
                                                    <i class="fa fa-times color-red"></i>
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenWeiXin == 1)
                                                {
                                                    if (p.HasWeiXin == 1)
                                                    {
                                                        <i class="fa fa-check color-green"></i>
                                                    }
                                                    else
                                                    {
                                                        <i class="fa fa-times color-red"></i>
                                                    }
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenWeiXin == 1)
                                                {
                                                    <span class="permillage">@( (p.InCost * 1000).ToString("f2"))</span>@:‰
                                            }
                                            </td>
                                            @* <td class="center">
                                                    @if (p.CanOpenWeiXin == 1)
                                                    {
                                                        <span class="permillage">@( (p.Cost * 1000).ToString("f2"))</span>@:‰
                                                    }
                                                </td>*@
                                            <td class="center">
                                                @if (p.CanOpenWeiXin == 1)
                                                {
                                                    <span class="permillage">@( (p.BankCost * 1000).ToString("f2"))</span>@:‰
                                            }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenWeiXin == 1)
                                                {
                                                    @(p.MinCost.ToMoney() + " - " + p.MaxCost.ToMoney())
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenWeiXin == 1)
                                                {
                                                    @(p.SNum.ToMoney() + " - " + p.ENum.ToMoney())
                                                }
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="center">银联</td>
                                            <td class="center">
                                                @if (p.CanOpenBank == 1)
                                                {
                                                    <i class="fa fa-check color-green"></i>
                                                }
                                                else
                                                {
                                                    <i class="fa fa-times color-red"></i>
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenBank == 1)
                                                {
                                                    if (p.HasBank == 1)
                                                    {
                                                        <i class="fa fa-check color-green"></i>
                                                    }
                                                    else
                                                    {
                                                        <i class="fa fa-times color-red"></i>
                                                    }
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenBank == 1)
                                                {
                                                    <span class="permillage">@( (p.InCost3 * 1000).ToString("f2"))</span>@:‰
                                            }
                                            </td>
                                            @*<td class="center">
                                                    @if (p.CanOpenBank == 1)
                                                    {
                                                        <span class="permillage">@( (p.Cost3 * 1000).ToString("f2"))</span>@:‰
                                                    }
                                                </td>*@
                                            <td class="center">
                                                @if (p.CanOpenBank == 1)
                                                {
                                                    <span class="permillage">@( (p.BankCost3 * 1000).ToString("f2"))</span>@:‰
                                            }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenBank == 1)
                                                {
                                                    @(p.MinCost3.ToMoney() + " - " + p.MaxCost3.ToMoney())
                                                }
                                            </td>
                                            <td class="center">
                                                @if (p.CanOpenBank == 1)
                                                {
                                                    @(p.BankSNum.ToMoney() + " - " + p.BankENum.ToMoney())
                                                }
                                            </td>
                                        </tr>
                                    }
                                }
                                else
                                {
                                    <tr>
                                        <td colspan="20" style="text-align: center; width: 100%; height: 100%; padding: 50px 0;">
                                            <img src="/Content/Manage/images/@(Request.QueryString["IsFirst"] == null ? "Index_back_img" : "Index_back_img_to").png" />
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    @* @Html.Partial("~/Areas/Manage/Views/Shared/Page.cshtml", FastPayWayList)*@
                </div>
            </div>
            <!-- END EXAMPLE TABLE PORTLET-->
        </div>
    </div>
    </div>
