                                    <option value="1" @(FastPayWay.TimeType == 1 ? " selected" : "")>按每天时段</option>
                                </select>
                            </div>
                            <p class="help-block"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-3">限制时间段 <span class="required"> * </span></label>
