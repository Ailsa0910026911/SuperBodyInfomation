@{
    List<OrderCashLog> OrderCashLogList = ViewBag.OrderCashLogList;
    this.Layout = null;
}
<table class="table table-striped table-bordered table-hover tablethc" id="OrderCashLog">
    <thead>
        <tr>
            <th>操作 </th>
            <th>操作人 </th>
            <th>备注 </th>
            <th>添加时间 </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in OrderCashLogList)
        {
        <tr class="odd gradeX">
            <td class="center">
                @switch (p.LogType)
                {
                    case 1:
                        @:付款
                        break;
                    case 2:
                        @:申请退款
                        break;
                    case 3:
                        @:审核退款
                        break;
                    case 4:
                        @:审核已付款
                        break;
                } 
            </td>
            <td class="center">@(p.AdminName) </td>
            <td class="center">@(p.Remark) </td>
            <td class="center">@(p.AddTime.ToString("yyyy-MM-dd HH:mm")) </td>
        </tr>
        }
    </tbody>
</table>

