@model PagedList<CTModel.BusinessShareProfit>
@using CTCommon.Extended
@using Webdiyer.WebControls.Mvc
@{
    CTModel.BusinessShareProfit bs = new CTModel.BusinessShareProfit();
    if(ViewBag.BusinessShareProfit!=null&& ViewBag.BusinessShareProfit!="")
    {
        bs = ViewBag.BusinessShareProfit;
    }
    ViewBag.Title = "分销分润百分比列表";
    Layout = "~/Views/_LayoutPage.cshtml";
}
<div class="panel panel-default">
    <div class="panel-body">
        <fieldset>
            @using (Ajax.BeginForm("DistributionFenRun", new RouteValueDictionary { { "id", "" } }, new AjaxOptions { UpdateTargetId = "articles", HttpMethod = "Post", InsertionMode = InsertionMode.Replace }, new RouteValueDictionary { { "id", "searchForm" }, { "class", "form-inline well" } }))
            {
                <input type="submit" value="添加" accesskey="S" class="btn btn-search" />            }
        </fieldset>
        <div id="articles">
            @Html.Partial("_DistributionFenRunTableList", Model)
        </div>
        @section scripts
{
            @{Html.RegisterMvcPagerScriptResource();}
            <script src="~/scripts/jquery.unobtrusive-ajax.min.js"></script>
        }
    </div>
</div>
